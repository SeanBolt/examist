{
  "name": "client",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.2.0",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.1",
    "redux-pending": "^0.1.2",
    "redux-promise": "^0.5.1",
    "redux-simple-router": "^2.0.4",
    "reselect": "^2.0.2",
    "superagent": "^1.7.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-lodash": "^2.0.1",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-plugin-transform-function-bind": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-regenerator": "^6.4.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "mocha": "^2.4.5",
    "nock": "^7.0.2",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "regenerator": "^0.8.42",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server -d --hot --inline --history-api-fallback",
    "lint": "eslint src",
    "test": "NODE_ENV=development,test mocha --compilers scss:./test/library/null-compiler.js,js:babel-core/register,jsx:babel-core/register --require ./node_modules/regenerator/runtime.js ./test/src/**/*.js",
    "build-test": "webpack test/index.js /tmp/test-examist.js --config webpack.test.config.js --target node"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "globals": {
      "__DEV__": true
    },
    "rules": {
      "strict": 0,
      "new-cap": 0,
      "no-shadow": 0,
      "curly": 0,
      "eol-last": 0,
      "comma-spacing": 0,
      "no-unused-vars": [
        1,
        {
          "varsIgnorePattern": "^_"
        }
      ],
      "no-use-before-define": 0,
      "no-trailing-spaces": 0,
      "no-undef": 2,
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false
        }
      ],
      "react/display-name": 0,
      "jsx-quotes": 1,
      "react/jsx-no-undef": 1,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 0,
      "react/wrap-multilines": 1,
      "no-console": 0
    },
    "ecmaFeatures": {
      "jsx": true
    },
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "mocha": true
    }
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-export-extensions",
      "transform-class-properties",
      "transform-decorators-legacy",
      "transform-function-bind",
      "transform-regenerator",
      "transform-object-rest-spread",
      "lodash"
    ]
  },
  "author": "Adrian Cooney <cooney.adrian@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancooney/examist.git"
  }
}
